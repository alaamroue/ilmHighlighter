{
  "name": "ilmHighlighter",
  "displayName": "ilm Highlighter for ProMaIDes",
  "description": "Syntax Highlighting for .ilm ProMaIDes files",
  "icon": "images/logo.png",
  "publisher": "AlaaMroue",
  "repository": {
    "type": "git",
    "url": "https://github.com/alaamroue/ilmHighlighter"
  },
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "ilm",
      "aliases": ["ilm", "ilm"],
      "extensions": [".ilm"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ilm",
      "scopeName": "source.ilm",
      "path": "./syntaxes/ilm.tmLanguage.json"
    }]
  }
}
